[user]
	name = Collin Dutter
	email = collin@griptape.ai
[color]
	ui = true
[pull]
	rebase = true
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[rerere]
	enabled = true
[core]
	autocrlf = input
[init]
	defaultBranch = main
